    printf("Digite o nome da unidade federativa: "); 
    scanf("%s", UF);

    printf("Digite a populacao: ");
    scanf("%d",&populacao1);

    printf("Digite a area: ");
    scanf("%f",&area1);

    printf("Digite o PIB: ");
    scanf("%f",&pib1);

    printf("Digite a quantidade de pontos turistícos: ");
    scanf("%d",&pontosTuristicos1);

    printf ("\n===========DIGITE OS DADOS DA SEGUNDA CARTA===================\n");
    
    printf("Digite o nome da cidade: "); 
